{
    "manifest_version": 3,
    "name": "Open WhatsApp Link",
    "version": "1.0",
    "description": "Open WhatsApp link in Browser",
    "icons": {
        "16": "icon.png"
      },
    "content_scripts": [
    {
        "js": ["script.js"],
        "matches": [
        "https://api.whatsapp.com/*"
        ]
    }
    ]
  } 